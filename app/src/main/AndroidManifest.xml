<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.igorlb.instagram">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/icon_manager"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round_manager"
        android:theme="@style/Theme.Instagram">
        <activity
            android:name=".start.Initial"
            android:label="LoginRegister">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.Main"
            android:label="Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>
</manifest>